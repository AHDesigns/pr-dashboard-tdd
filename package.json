  "private": true,
  "dependencies": {
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "3.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "24.9.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4"
  },
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=build/ node build/server.js",
    "test": "react-scripts test",
    "build": "rm -rf build && yarn build:client && yarn build:server",
    "dev:client": "react-scripts start",
    "dev:server": "nodemon",
    "build:client": "react-scripts build",
    "build:server": "tsc --project tsconfig.build.json",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "lint:js": "eslint --ext ts,tsx ./src",
    "lint:fix": "yarn lint:js --fix",
    "lint:css": "echo \"not setup\"",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
